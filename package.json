{
  "name": "react-stable-uniqueid",
  "version": "1.2.1",
  "description": "A React component that provides a stable unique ID for accessibility, SVG effects, and other purposes",
  "main": "dist/index.js",
  "module": "dist-es6/index.js",
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "build:commonjs": "cross-env TARGET=commonjs babel src --ignore \"src/*.test.js\" --ignore \"src/stories/**\" --out-dir dist",
    "build:es6": "cross-env TARGET=esm babel src --ignore \"src/*.test.js\" --ignore \"src/stories/**\" --out-dir dist-es6",
    "build": "npm run build:commonjs && npm run build:es6",
    "storybook": "start-storybook -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dallonf/react-stable-uniqueid.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "uniqueid",
    "lodash",
    "unique",
    "svg",
    "accessibility"
  ],
  "author": "Dallon Feldner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dallonf/react-stable-uniqueid/issues"
  },
  "homepage": "https://github.com/dallonf/react-stable-uniqueid#readme",
  "peerDependencies": {
    "react": "15.x || 16.x || 17.x",
    "prop-types": "15.x || 16.x || 17.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/react": "^6.3.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/example/"
    ]
  },
  "dependencies": {
    "lodash.uniqueid": "^4.0.1",
    "react-hoc": "^0.1.8"
  }
}
