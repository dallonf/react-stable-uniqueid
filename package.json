{
  "name": "react-stable-uniqueid",
  "version": "2.0.0",
  "description": "A React component that provides a stable unique ID for accessibility, SVG effects, and other purposes",
  "main": "dist/index.js",
  "module": "dist-es6/index.js",
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "build:commonjs": "cross-env TARGET=commonjs babel src --ignore \"src/*.test.js\" --ignore \"src/stories/**\" --out-dir dist",
    "build:es6": "cross-env TARGET=esm babel src --ignore \"src/*.test.js\" --ignore \"src/stories/**\" --out-dir dist-es6",
    "build": "npm run build:commonjs && npm run build:es6",
    "storybook": "start-storybook -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dallonf/react-stable-uniqueid.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "uniqueid",
    "lodash",
    "unique",
    "svg",
    "accessibility"
  ],
  "author": "Dallon Feldner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dallonf/react-stable-uniqueid/issues"
  },
  "homepage": "https://github.com/dallonf/react-stable-uniqueid#readme",
  "peerDependencies": {
    "prop-types": "16.x || 17.x",
    "react": "16.x || 17.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "raf": "^3.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/example/"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest-setup.js"
    ]
  },
  "dependencies": {
    "lodash.uniqueid": "^4.0.1",
    "react-hoc": "^0.1.8"
  }
}
